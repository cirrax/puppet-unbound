stub-zone:
  name: <%= @title %>
<%- if not @stub_host.nil? -%>
  <%- @stub_host.each do |v| -%>
  stub-host: <%= v %>
  <%- end -%>
<%- end -%>
<%- if not @stub_addr.nil? -%>
  <%- @stub_addr.each do |v| -%>
  stub-addr: <%= @stub_addr %>
  <%- end -%>
<%- end -%>
<%- if not @stub_prime.nil? -%>
  stub-prime: <%= scope.call_function('bool2str', [@stub_prime, 'yes', 'no']) %>
<%- end -%>
<%- if not @stub_first.nil? -%>
  stub-first: <%= scope.call_function('bool2str', [@stub_first, 'yes', 'no']) %>
<%- end -%>
<%- if not @stub_ssl_upstream.nil? -%>
  stub-ssl-upstream: <%= scope.call_function('bool2str', [@stub_ssl_upstream, 'yes', 'no']) %>
<%- end -%>
